{
  "configurations": [
    {
      "name": "Debug Client Program",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/client/src/index.js"
    }, {
      "name": "Debug Server Program",
      "program": "${workspaceFolder}/server/index.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    }
  ]
}